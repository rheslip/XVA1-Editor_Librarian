        <luaMethod luaMethodName="PatchAssignValues" luaMethodCode="-- This method assigns voice data from a memory block &#13;-- to all modulators in the panel&#13;&#10;-- called when we receive a MIDI sysex patch dump message from the synth&#13;&#10;&#13;function PatchAssignValues (data, midi)&#13;    local midiSize = data:getSize()  -- 1028 bytes for the XVA1 which includes header&#13;&#10;    local SysexHeaderSize=5  -- address of first parameter byte&#13;&#10;    local i,j&#13;&#10;    local midiParam,paramnumber,mod,name&#13;&#10;&#9;local oscsync,oscmode,loop,loopat&#13;&#10;    local unit= panel:getGlobalVariable(0) -- unit number&#13;&#10;&#9;local x1,x2,x3,x4,su,s1,s2,s3,s4&#13;&#10;&#13;&#10;&#13;&#10;    for i = SysexHeaderSize,(midiSize-2),2 do -- gets the voice parameter values (2 bytes per parameter &#13;        midiParam= data:getByte(i)*128  -- first byte is upper 2 bits&#13;&#10;        midiParam=midiParam+data:getByte(i+1)  -- low 7 bits&#13;&#10;        paramnumber = (i - SysexHeaderSize)/2+1 -- index of the parameter&#13;&#10;&#13;&#10;console(string.format(&quot;%d&quot;, midiParam))&#13;&#10;&#13;        name = string.format(&quot;%d&quot;, paramnumber) -- find the modulator&#13;        mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10; --   &#9;local symbols = {&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot;|&quot;,&quot; &quot;,&quot;!&quot;,&quot;\&quot;&quot;,&quot;#&quot;,&quot;$&quot;,&quot;%&quot;,&quot;&amp;&quot;,&quot;'&quot;,&quot;(&quot;,&quot;)&quot;,&quot;*&quot;,&quot;+&quot;,&quot;,&quot;,&quot;-&quot;,&quot