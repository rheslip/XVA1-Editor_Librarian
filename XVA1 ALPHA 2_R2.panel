n&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p5bit2&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,4) == 4 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p5bit3&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,8) == 8 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;&#13;&#10;&#9;&#9;elseif paramnumber == 6 then&#13;&#10;&#9;&#9;&#9;oscmode=midiParam&#9;&#9;-- save bits to update globals&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p6bit0&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,1) == 1 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p6bit1&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,2) == 2 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p6bit2&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,4) == 4 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p6bit3&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,8) == 8 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#9;&#9;elseif paramnumber == 145 then  -- env gen loop bits&#13;&#10;&#9;&#9;&#9;loop=midiParam&#9;&#9;-- save OP bits to update globals&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p145bit0&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,1) == 1 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p145bit1&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,2) == 2 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p145bit2&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,4) == 4 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;&#13;&#10;&#9;&#9;elseif paramnumber == 146 then  -- env gen loop seg bits&#13;&#10;&#9;&#9;&#9;loopat=midiParam&#9;&#9;-- save OP bits to update globals&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p146bit0&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,1) == 1 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p146bit1&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,2) == 2 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;        &#9;mod = panel:getModulatorByName(&quot;p146bit2&quot;)&#13;&#10;            if mod ~= nil then&#13;&#10;&#9;&#9;&#9; &#9;if bit.band(midiParam,4) == 4 then&#13;&#10;               &#9;&#9;mod:setModulatorValue(1, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;else mod:setModulatorValue(0, false, false, false)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;            end&#13;&#10;&#13;&#10;-- still a few bitmaps to add &#13;&#10;&#13;&#10;&#9;&#9;else&#13;&#10;        &#9;name = string.format(&quot;%d&quot;, paramnumber) -- find the modulator&#13;        &#9;mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;            if mod ~= nil then&#13;&#10;               mod:setModulatorValue(midiParam, false, false, false)&#13;&#10;--local test=string.format(&quot;%d&quot;, paramnumber)&#13;&#10;--console(test)&#13;&#10;            end&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;-- send each parameter as a sysex message to the synth when patch is loaded from a file &#13;&#10;-- explanation for this logic:&#13;&#10;-- first - there is no sysex message to load a complete patch in the XFM2/XVA1 - must be sent by individual parameter change messages&#13;&#10;--  when you change the UI values in the above loop CTRLR will trigger modulatorchanged() which&#13;&#10;--  would normally send individual parameter update messages. The global Updatemidi is used to prevent this from happening when &#13;&#10;-- we load parameters from a memory block&#13;&#10;-- if modulatorchanged is allowed to send messages the bitmapped parameters get messed up with all the setting and resetting of bits&#13;&#10;-- and it would also send 6 or 7 sysex messages for each bitmap which is not very efficient&#13;&#10;-- when we load parameters from memory we update the bitmaps as one parameter to avoid these problems&#13;&#10;-- note we also have to send the unit address which is always 0 in a sysex file&#13;&#10;&#13;&#10;&#9;&#9;if midi == true then&#13;&#10;    &#9;&#9;x1=0&#13;&#10;&#13;&#10;    &#9;&#9;if paramnumber &gt; 127 then x1 = 1 end -- parameter address can but up to 511&#13;&#10;      &#9;&#9;if paramnumber &gt; 255 then x1 = 2 end&#13;&#10;    &#9;&#9;if paramnumber &gt; 383 then x1 = 3 end&#13;&#10;&#13;&#10;    &#9;&#9;x2= paramnumber%128&#13;&#10;&#13;&#10;    &#9;&#9;x3=0&#13;&#10;&#13;&#10;    &#9;&#9;if midiParam &gt; 127 then x3 = 1 end -- values are 255 or less&#13;&#10;    &#9;&#9;x4= midiParam %128&#13;&#10;&#13;&#10;    &#9;&#9;su=string.format(&quot;%.2x&quot;,unit)&#13;&#10;    &#9;&#9;s1 = string.format(&quot;%.2x&quot;,x1)&#13;&#10;    &#9;&#9;s2 = string.format(&quot;%.2x&quot;,x2)&#13;&#10;    &#9;&#9;s3 = string.format(&quot;%.2x&quot;,x3)&#13;&#10;    &#9;&#9;s4 = string.format(&quot;%.2x&quot;,x4)&#13;&#10;-- had to use table.concat here, sending a string does not work&#13;&#10;    &#9;&#9;sysex = table.concat({&quot;f0 44&quot;,su,s1,s2,s3,s4, 'F7'}, ' ')&#13;&#10;--console(sysex)&#13;&#10;-- method below does not send midi if you use a formatted string - could be a bug in post 5.2.301 releases&#13;&#10;&#13;&#10;&#9;&#9;&#9;panel:sendMidiMessageNow(CtrlrMidiMessage(sysex))&#13;&#10;&#9;&#9;end&#13;&#10;            &#13;&#10;    end&#13;&#10;&#13;&#10;-- update the globals which hold the unit number and the bitmapped parameters&#13;&#10;&#9;panel:setProperty(&quot;panelGlobalVariables&quot;,unit..&quot;:&quot;..oscsync..&quot;:&quot;..oscmode..&quot;:&quot;..loop..&quot;:&quot;..loopat, false)&#13;&#10;&#13;&#10;&#13;&#10;--&#9;SetInfoLabelVisibility(false)&#13;&#10;--&#9;SetPatchNameVisibility(true)&#13;end&#13;"
