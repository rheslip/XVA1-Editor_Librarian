        <luaMethod luaMethodName="PatchAssembleValues" luaMethodCode="-- This method assembles the param values from &#13;-- all modulators and stores them in a memory block&#13;&#10;-- used for saving program as a sysex file&#13;&#10;&#13;function PatchAssembleValues()&#13;&#10;    local SysexSize = 1028  -- size of a sysex program dump&#13;    local data = MemoryBlock(SysexSize, true)&#13;--    local prog= panel:getModulatorByName(&quot;program&quot;):getModulatorValue()&#13;&#10;&#13;&#10;    local SysexHeaderSize=5&#13;&#10;    local i,mod, paramvalue, paramnumber&#13;&#10;&#13;&#10;&#9;data:setByte(0, 0xF0) -- sysex header&#13;&#10;&#9;data:setByte(1, 0x43)&#13;&#10;&#9;data:setByte(2, 0x0)  -- unit always 0&#13;&#10;&#9;data:setByte(3, 0x0)  -- program number always 0&#13;&#10;&#9;data:setByte(4, 0x0)  -- &#13;&#10;&#13;&#10;    for i = SysexHeaderSize,(SysexSize-2),2 do -- run through all modulators and fetch their value&#13;&#10;        paramnumber = (i - SysexHeaderSize)/2+1 -- index of the parameter&#13;&#10;&#13;        name = string.format(&quot;%d&quot;, paramnumber) -- find the modulator&#13;&#10;&#13;        mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;&#13;&#10;        if mod ~= nil then&#13;&#10;        &#9;paramvalue=mod:getModulatorValue()&#13;&#10;&#9;&#9;&#9;if paramvalue &gt; 255 then paramvalue = 255 end -- sanity checks - only 8 bit values&#13;&#10;&#9;&#9;&#9;if paramvalue &lt;0 then paramvalue =0 end&#13;&#10;&#9;&#9;&#9;if paramvalue &gt;127 then &#13;&#10;&#9;&#9;&#9;&#9;data:setByte(i, 1 )&#13;&#10;&#9;&#9;&#9;&#9;data:setByte(i+1,paramvalue-128)&#13;&#10;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;data:setByte(i, 0)&#13;            &#9;data:setByte(i+1, paramvalue ) -- parameter value is split into MSB LSB format&#13;&#10;&#9;&#9;&#9;end&#13;&#10;--console(string.format(&quot; %d&quot;,paramvalue))&#13;&#10;    &#9;else&#13;&#10;            data:setByte(i, 0 ) -- put zeros if no valid parameter found&#13;&#10;            data:setByte(i+1, 0 ) &#13;&#10;-- console(&quot;0&quot;)  &#13;&#10;--console(string.format(&quot; %d not found&quot;,paramnumber)) &#9;&#9;&#9;&#13;&#9;&#9;end&#13;&#10;    end&#13;&#10;--console (&quot;looped thru modulators&quot;)&#13;&#10;-- stuff in the globals used for bitmapped parameters etc&#13;&#10;-- parameter offset in the data block is SysexHeaderSize + (parameter number -1) * 2&#13;&#10;&#13;&#10;&#9;data:setByte(SysexHeaderSize,0x0)  -- unit number always 0 in file&#13;&#10;&#13;&#10;&#9;data:setByte(SysexHeaderSize+8,0)&#13;&#10;&#9;data:setByte(SysexHeaderSize+9,panel:getGlobalVariable(1)) -- sync bits&#13;&#10;&#9;data:setByte(SysexHeaderSize+10,0)&#13;&#10;&#9;data:setByte(SysexHeaderSize+11,panel:getGlobalVariable(2)) -- osc mode bits&#13;&#10;&#9;data:setByte(SysexHeaderSize+288,0)&#13;&#10;&#9;data:setByte(SysexHeaderSize+289,panel:getGlobalVariable(3)) -- env gen loop bits&#13;&#10;&#9;data:setByte(SysexHeaderSize+290,0)&#13;&#10;&#9;data:setByte(SysexHeaderSize+291,panel:getGlobalVariable(4)) -- env gen loop seg bits&#13;&#10;&#13;&#10;-- stuff in reasonable values for parameters that are not set by the panel&#13;&#10;&#9;data:setByte(SysexHeaderSize+292,0) -- EG restart - not sure what it does&#13;&#10;&#9;data:setByte(SysexHeaderSize+293,0)&#13;&#10;&#9;data:setByte(SysexHeaderSize+586,1) -- early reflections dry - I don't modify it&#13;&#10;&#9;data:setByte(SysexHeaderSize+587,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+598,1) -- delay dry - I don't modify it&#13;&#10;&#9;data:setByte(SysexHeaderSize+599,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+618,1) -- phaser dry - I don't modify it&#13;&#10;&#9;data:setByte(SysexHeaderSize+619,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+718,1) -- chorus dry - I don't modify it&#13;&#10;&#9;data:setByte(SysexHeaderSize+719,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+778,1) -- reverb dry - I don't modify it&#13;&#10;&#9;data:setByte(SysexHeaderSize+779,127)&#13;&#10;&#13;&#10;-- unmapped registers in the envelope generator range - init patch values from the XVA1&#13;&#10;&#9;data:setByte(SysexHeaderSize+174,1)   -- param 88&#13;&#10;&#9;data:setByte(SysexHeaderSize+175,127) -- init value is 255&#13;&#10;&#9;data:setByte(SysexHeaderSize+176,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+177,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+184,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+185,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+186,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+187,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+194,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+195,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+196,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+197,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+234,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+235,94)  -- init value is 220&#13;&#10;&#9;data:setByte(SysexHeaderSize+236,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+237,94)&#13;&#10;&#9;data:setByte(SysexHeaderSize+244,1)   -- param 123&#13;&#10;&#9;data:setByte(SysexHeaderSize+245,127) -- init value is 255&#13;&#10;&#9;data:setByte(SysexHeaderSize+246,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+247,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+254,1)   -- param 128&#13;&#10;&#9;data:setByte(SysexHeaderSize+255,127) -- init value is 255&#13;&#10;&#9;data:setByte(SysexHeaderSize+256,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+257,127) &#13;&#10;&#9;data:setByte(SysexHeaderSize+274,1)   -- param 138&#13;&#10;&#9;data:setByte(SysexHeaderSize+275,127) -- init value is 255&#13;&#10;&#9;data:setByte(SysexHeaderSize+276,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+277,127)&#13;&#10;&#9;data:setByte(SysexHeaderSize+264,1)   -- param 133&#13;&#10;&#9;data:setByte(SysexHeaderSize+265,32) -- init value is 160&#13;&#10;&#9;data:setByte(SysexHeaderSize+266,1) &#13;&#10;&#9;data:setByte(SysexHeaderSize+267,32) &#13;&#10;&#13;&#10;&#9;data:setByte(SysexSize - 1, 0xF7) -- end of sysex&#13;&#10;&#13;&#10;-- debug - dump all the parameters&#13;&#10;    for i = SysexHeaderSize,(midiSize-2),2 do -- gets the voice parameter values (2 bytes per parameter &#13;        midiParam= data:getByte(i)*128  -- first byte is upper 2 bits&#13;&#10;        midiParam=midiParam+data:getByte(i+1)  -- low 7 bits&#13;&#10;        paramnumber = (i - SysexHeaderSize)/2+1 -- index of the parameter&#13;&#10;console(string.format(&quot;%d&quot;, midiParam))&#13;&#10;&#9;end&#13;&#10;&#13;&#10;    return data&#13;end&#13;&#10;"
