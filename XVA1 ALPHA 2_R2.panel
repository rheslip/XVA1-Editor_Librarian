      <luaMethodGroup name="UI_modulators" uuid="cc5af3182b234ddc8fe69b826f9c7fd6">
        <luaMethod luaMethodName="modulatorchanged" luaMethodCode="--&#13;&#10;-- Called when a modulator value changes&#13;&#10;-- @mod   http://ctrlr.org/api/class_ctrlr_modulator.html&#13;&#10;-- @value    new numeric value of the modulator&#13;&#10;--&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;modulatorchanged = function(--[[ CtrlrModulator --]] modulator, --[[ number --]] value)&#13;&#10;    local paramAddress,x1,x2,x3,x4&#13;&#10;    local unit= panel:getGlobalVariable(0) -- unit number&#13;&#10;&#13;&#10;&#9;paramAddress = tonumber(modulator:getProperty(&quot;modulatorCustomName&quot;))&#13;&#10;    x1=0&#13;&#10;&#13;&#10;    if paramAddress &gt; 127 then x1 = 1 end&#13;&#10;    if paramAddress &gt; 255 then x1 = 2 end&#13;&#10;    if paramAddress &gt; 383 then x1 = 3 end&#13;&#10;&#13;&#10;    x2= paramAddress%128&#13;&#10;&#13;&#10;    x3=0&#13;&#10;&#13;&#10;    if value &gt; 127 then x3 = 1 end&#13;&#10;    if value &gt; 255 then x3 = 2 end&#13;&#10;    if value &gt; 383 then x3 = 3 end&#13;&#10;&#13;&#10;    x4= value %128&#13;&#10;&#13;&#10;    su=string.format(&quot;%.2x&quot;,unit)&#13;&#10;    s1 = string.format(&quot;%.2x&quot;,x1)&#13;&#10;    s2 = string.format(&quot;%.2x&quot;,x2)&#13;&#10;    s3 = string.format(&quot;%.2x&quot;,x3)&#13;&#10;    s4 = string.format(&quot;%.2x&quot;,x4)&#13;&#10;-- had to use table.concat here, sending a string does not work&#13;&#10;    sysex = table.concat({&quot;f0 44&quot;,su,s1,s2,s3,s4, 'F7'}, ' ')&#13;&#10;--console(sysex)&#13;&#10;-- method below does not send midi if you use a formatted string&#13;&#10;&#9;if Updatemidi then&#13;&#10;&#9;&#9;panel:sendMidiMessageNow(CtrlrMidiMessage(sysex))&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#13;&#10;end&#13;&#10;"
                   luaMethodLinkedProperty="" luaMethodSource="0" uuid="aa2f3c091f6c4fa389d947cbc30767dc"
                   luaMethodValid="1"/>
        <luaMethod luaMethodName="setParamfromBitmask" luaMethodCode="--&#13;&#10;-- Called when a modulator value changes&#13;&#10;-- @mod   http://ctrlr.org/api/class_ctrlr_modulator.html&#13;&#10;-- @value    new numeric value of the modulator 0 or 1&#13;&#10;-- the modulators index property holds a bitmask to set/clear&#13;&#10;-- used to modify bitmapped parameters e.g. Algorithm&#13;&#10;--&#13;&#10;&#13;&#10;setParamfromBitmask = function(--[[ CtrlrModulator --]] modulator, --[[ number --]] value)&#13;&#10;    local paramAddress,x1,x2,x3,x4, bitmask&#13;&#10;    local unit=panel:getGlobalVariable(0) -- unit number&#13;&#10;&#13;&#10;&#9;junk=bitmaps[1]&#13;&#10;&#9;paramAddress = tonumber(modulator:getProperty(&quot;modulatorCustomName&quot;))&#13;&#10;&#13;&#10;local test=string.format(&quot;%d&quot;, paramAddress)&#13;&#10;console(test)&#13;&#10;&#13;&#10;&#9;bitmask = tonumber(modulator:getProperty(&quot;modulatorCustomIndex&quot;))&#13;&#10;&#13;&#10;-- create sysex string&#13;&#10;-- set parameter address&#13;&#10;    x1=0&#13;&#10;&#13;&#10;    if paramAddress &gt; 127 then x1 = 1 end&#13;&#10;    if paramAddress &gt; 255 then x1 = 2 end&#13;&#10;    if paramAddress &gt; 383 then x1 = 3 end&#13;&#10;&#13;&#10;    x2= paramAddress%128&#13;&#10;&#13;&#10;    x3=0&#13;&#10;&#13;&#10;    if value &gt; 0 then&#13;&#10;        bitParameters[paramAddress]=bit.bor(bitParameters[paramAddress],bitmask)&#13;&#10;    else&#13;&#10;       bitmask=bit.bxor(bitmask,255) -- invert mask&#13;&#10;       bitParameters[paramAddress]=bit.band(bitParameters[paramAddress],bitmask)&#13;&#10;    end&#13;&#10;&#13;&#10;    x4=bitParameters[paramAddress]&#13;&#10;&#13;&#10;    su=string.format(&quot;%.2x&quot;,unit)&#13;&#10;    s1 = string.format(&quot;%.2x&quot;,x1)&#13;&#10;    s2 = string.format(&quot;%.2x&quot;,x2)&#13;&#10;    s3 = string.format(&quot;%.2x&quot;,x3)&#13;&#10;    s4 = string.format(&quot;%.2x&quot;,x4)&#13;&#10;-- had to use table.concat here, sending a string does not work&#13;&#10;    sysex = table.concat({&quot;f0 44&quot;,su,s1,s2,s3,s4, 'F7'}, ' ')&#13;&#10;--console(sysex)&#13;&#10;-- method below does not send midi if you use a formatted string&#13;&#10;&#9;if Updatemidi then&#13;&#10;&#9;&#9;panel:sendMidiMessageNow(CtrlrMidiMessage(sysex))&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#13;&#10;end&#13;&#10;&#13;&#10;"
