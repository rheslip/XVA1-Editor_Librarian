        <luaMethod luaMethodName="PatchInit" luaMethodCode="-- set current program to default values&#13;function PatchInit (modulator, newValue)&#13;    -- This variable stops index issues during panel bootup&#13;    if panel:getRestoreState() == true or panel:getProgramState() == true then&#13;        return&#13;    end&#13;&#13;    if panel_loaded == 0 then&#13;        return&#13;    end&#13;&#13;&#10;    local unit=panel:getGlobalVariable(0) -- unit number&#13;&#10;&#13;&#10;    local su=string.format(&quot;%.2x&quot;,unit)&#13;&#10;&#13;&#10;-- had to use table.concat here, sending a string does not work&#13;&#10;    local sysex = table.concat({&quot;f0 44&quot;,su,&quot;04 00 00 00 F7&quot;}, ' ')&#13;&#10;--console(sysex)&#13;&#10;-- method below does not send midi if you use a formatted string&#13;&#10;    panel:sendMidiMessageNow(CtrlrMidiMessage(sysex))&#13;&#10;&#13;&#10;end&#13;&#10;"
