        <luaMethod luaMethodName="pasteEnvelope" luaMethodCode="--&#13;&#10;-- Called when a paste button is pressed&#13;&#10;-- @mod   http://ctrlr.org/api/class_ctrlr_modulator.html&#13;&#10;-- @value    new numeric value of the modulator&#13;&#10;--&#13;&#10;-- the env gen parameters numbers are spread out but they are the same offsets from each other&#13;&#10;-- each paste button custom name is the parameter base address + 2000 &#13;&#10;-- so it won't conflict with the other parameter numbers which are &lt; 512&#13;&#10;&#13;&#10;&#13;&#10;pasteEnvelope = function(--[[ CtrlrModulator --]] modulator, --[[ number --]] value)&#13;&#10;    local envbase, name, mod&#13;&#10;&#13;&#10;&#9;envbase = tonumber(modulator:getProperty(&quot;modulatorCustomName&quot;)) -- base address of envelope parameters +1000 so it doesn't clash with other parameters&#13;&#10;&#9;envbase=envbase-2000&#13;&#10;&#13;&#10;--local test=string.format(&quot;%d&quot;, envbase)&#13;&#10;--console(test)&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase) -- find parameter value for that env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(L0buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, L0buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#9;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+5) -- find parameter value for that env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(L1buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, L1buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+10) -- find parameter value  env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(L2buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, L2buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#9;&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+15) -- find parameter value  env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(L3buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, L3buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+20) -- find parameter value  env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(L4buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, L4buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+25) -- find parameter value  env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(L5buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, L5buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+35) -- find parameter value  env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(R1buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, R1buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+40) -- find parameter value  env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(R2buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, R2buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+45) -- find parameter value  env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(R3buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, R3buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+50) -- find parameter value  env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(R4buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, R4buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#13;&#10;    name = string.format(&quot;%d&quot;, envbase+55) -- find parameter value  env gen&#13;    mod = panel:getModulatorWithProperty(&quot;modulatorCustomName&quot;, name)&#13;&#10;    if mod ~= nil then&#13;&#10;         mod:setModulatorValue(R5buffer, false, true, false)&#13;&#10;--test=string.format(&quot;%d&quot;, R5buffer)&#13;&#10;--console(test)&#13;&#10;    end&#13;&#10;&#13;&#10;end"
                   luaMethodLinkedProperty="" luaMethodSource="0" uuid="4e253a4fe89d47e1940f010a83376cbf"
                   luaMethodValid="1"/>
        <luaMethod luaMethodName="Patchsavemenu" luaMethodCode="function Patchsavemenu (mod, value)&#13;    -- This variable stops index issues during panel bootup&#13;    if panel:getRestoreState() == true or panel:getProgramState() == true then&#13;        return&#13;    end&#13;&#13;&#9;if value == -1 then&#13;&#9;&#9;return&#13;&#9;end&#13;&#13;    if panel_loaded == 0 then&#13;        return&#13;    end&#13;&#10;&#13;&#10;&#13;&#10;    if value == 0 then  -- save to XFM2 memory&#13;&#10;&#9;&#9;local&#9;writeslot= panel:getModulatorByName(&quot;write_slot&quot;):getModulatorValue()&#13;&#10;--console(&quot;write &quot;..writeslot)&#13;&#10;&#13;&#10;&#9;&#9;if writeslot &lt;0 then writeslot=127 end -- write to last slot if number is bogus&#13;&#10;&#9;&#9;if writeslot &gt; 127 then writeslot=127 end &#13;&#10;&#13;&#10;&#9;&#9;s4 = string.format(&quot;%.2x&quot;,writeslot)&#13;&#10;&#9;&#9;&#13;&#10;    &#9;local ret = AlertWindow.showOkCancelBox(AlertWindow.InfoIcon, &quot;Write to XFM2 memory&quot;, &quot;Save patch to the selected write slot on XFM2?&quot;, &quot;OK&quot;, &quot;Cancel&quot;)&#13;    &#9;if ret == false then return end&#13;&#10;&#13;&#10;    &#9;local unit=panel:getGlobalVariable(0) -- unit number&#13;&#10;&#13;&#10;    &#9;local su=string.format(&quot;%.2x&quot;,unit)&#13;&#10;&#13;&#10;-- had to use table.concat here, sending a string does not work&#13;&#10;    &#9;local sysex = table.concat({&quot;f0 43&quot;,su,&quot;04 01 00 &quot;,s4,&quot; F7&quot;}, ' ')&#13;&#10;--console(sysex)&#13;&#10;-- method below does not send midi if you use a formatted string&#13;&#10;    &#9;panel:sendMidiMessageNow(CtrlrMidiMessage(sysex))&#13;&#10;&#13;&#10;&#9;else&#10;&#13;    &#9;f = utils.saveFileWindow (&quot;Save patch&quot;, File(&quot;&quot;), &quot;*.syx&quot;, true)&#10;    &#9;if f:isValid() == false then&#10;        &#9;return&#10;    &#9;end&#10;    &#9;f:create()&#10;    &#9;if f:existsAsFile() then&#13;&#10;--console (&quot;file created&quot;)&#10;        &#9;local PatchDataCurrent = PatchAssembleValues()&#13;&#10;--&#9;&#9;Voice_CalculateChecksum(PatchDataCurrent)&#13;&#10;--console (&quot;patch assembled&quot;)&#13;&#10;        -- Check if the file exists&#10;        &#9;if f:existsAsFile() == false then&#10;            -- If file does not exist, then create it&#10;            &#9;if f:create() == false then&#10;                -- If file cannot be created, then fail here&#10;                &#9;utils.warnWindow (&quot;\n\nSorry, the Editor failed to\nsave the patch to disk!&quot;, &quot;The file cannot be created.&quot;)&#10;                &#9;return&#10;            &#9;end&#10;        &#9;end&#10;        -- If we reached this point, we have a valid file we can try to write to&#10;        &#9;if f:replaceWithData (PatchDataCurrent) == false then&#10;            &#9;utils.warnWindow (&quot;File write&quot;, &quot;Sorry, the Editor failed to\nwrite the data to file!&quot;)&#10;        &#9;end&#10;-- console (&quot;File save complete, Voice saved to disk&quot;)&#10;    &#9;end&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;mod:setValue(-1, false)&#13;&#10;&#13;&#10;end"
                   luaMethodLinkedProperty="" luaMethodSource="0" uuid="7c8ef739d4374c2f91e66eb80de62947"
                   luaMethodValid="1"/>
